{
    "version": "0.2.0",
    "configurations": [
        {
            "type": "node",
            "request": "launch",
            "name": "Launch Dev",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "program": "${workspaceFolder}/src/index.js",
            "args": ["--dev"]
        },
        {
            "type": "node",
            "request": "launch",
            "name": "Launch Index",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "program": "${workspaceFolder}/src/index.js"
        },
        {
            "type": "node",
            "request": "launch",
            "name": "Launch Register Commands",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "program": "${workspaceFolder}/src/registerCommands.js"
        },
        {
            "type": "node",
            "request": "launch",
            "name": "Launch Renderer",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "program": "${workspaceFolder}/Flowcharts/render.js"
        },
        {
            "type": "node",
            "request": "launch",
            "name": "Launch Tests",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "program": "${workspaceFolder}/test.js"
        }
    ]
}